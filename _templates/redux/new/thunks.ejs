import { actions } from './redux.<%=h.changeCase.dot(name)%>';
import { Dispatch } from 'redux';

// eslint-disable-next-line prettier/prettier
const {
  fetch<%=h.changeCase.pascal(name)%>sAsync,
} = actions;

export const getAll<%=h.changeCase.pascal(name)%>sThunk = () => async (dispatch: Dispatch): Promise<void> => {
  dispatch(fetch<%=h.changeCase.pascal(name)%>sAsync.request());
  try {
    const response = await getAll<%=h.changeCase.pascal(name)%>s();
    dispatch(fetch<%=h.changeCase.pascal(name)%>sAsync.success(response.data.results));
  } catch (err) {
    dispatch(fetch<%=h.changeCase.pascal(name)%>sAsync.failure(err));
